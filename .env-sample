# ModCore UserBot Configuration
# Copy this file to .env and fill in your values

# Telegram API credentials (get from https://my.telegram.org)
API_ID=12345678
API_HASH=your_api_hash_here

# Session string (generate using generate_session.py)
SESSION_STRING=your_session_string_here

# Bot owner configuration
OWNER_ID=123456789
OWNER_USERNAME=your_username_here

# Optional: Log channel ID (for sending logs)
LOG_CHANNEL=-123456789

# Source code URL
SOURCE_CODE=https://github.com/aes-co/umodcore
